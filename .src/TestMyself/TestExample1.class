'' Tests to test UnitTest itself. Rename class to start with "_GuTest" to activate.
Inherits ATestContainer

Public Const constDegUnittestSelftest As Boolean = True

Public Sub SetupEach()

    If Me.Debug Then
        Print "Setup Each"
    Endif

End

Public Sub TeardownEach()

    If Me.Debug Then
        Print "Teardown Each"
    Endif

End

Public Sub TestStringNull()

    If Me.Debug Then
        Print "GuTestExample1::Test First Test"
    Endif
    Me.R.AssertEmpty(Null, "Diskussion ohne Ergebnis")

End

Public Sub TestStringOk()

    If Me.Debug Then
        Print "GuTestExample1::Test Second Test"
    Endif
    Me.R.AssertEqualsString("ja", "ja", "Diskussion")

End

