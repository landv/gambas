' Gambas class file
PRIVATE $sOld AS String

PUBLIC SUB Form_Open()

  Application.Theme = "gnome"
  PictureBox1.Picture = Picture["icon:/64/new"]

END

PUBLIC SUB Action_Activate(Key AS String) AS Boolean
  
  DEBUG Key
  
  Action[$sOld].Value = FALSE
  $sOld = Key
  Action[Key].Value = TRUE
  
END


PUBLIC SUB Button3_Click()

  Action["Test*"].Enabled = NOT Action["Test"].Enabled

END
