Public Sub DumpSession()
  
  Dim sKey As String

  Debug String$(80, "-")
  
  If Session.Id Then
    
    For Each sKey In Session.Keys
      Error sKey; ": "; JSON.Encode(Session[sKey])
    Next
    
  Endif
  
  Debug String$(80, "-")
  
End

