# Gambas Form File 2.0

{ Form Form
  MoveScaled(0,0,81,71)
  Text = ("Make executable")
  Icon = Picture["icon:/small/exec"]
  Persistent = True
  Arrangement = Arrange.Vertical
  Spacing = 8
  Padding = 8
  { fchExec FileChooser
    MoveScaled(2,1,68,45)
    Expand = True
    ShowBookmark = True
  }
  { Expander1 Expander
    MoveScaled(2,47,70,14)
    Arrangement = Arrange.Vertical
    Spacing = 8
    Text = ("Options")
    TextFont = Font["Bold"]
    Hidden = True
    { HBox2 HBox
      MoveScaled(0,0,68,3)
      Spacing = 8
      { Label1 Label
        MoveScaled(0,0,24,3)
        AutoResize = True
        Text = ("Run this command after")
      }
      { txtCommand TextBox
        MoveScaled(26,0,24,3)
        ToolTip = ("<b>$(FILE)</b> will be replaced by the path of the executable")
        Expand = True
        Text = ("")
      }
    }
    { VBox1 VBox
      MoveScaled(1,4,67,6)
      { chkDebug CheckBox
        MoveScaled(0,0,56,3)
        Text = ("Keep debugging information in executable")
      }
      { chkShortcut CheckBox
        MoveScaled(0,3,49,3)
        Text = ("")
      }
    }
  }
  { HBox1 HBox
    MoveScaled(2,66,73,3)
    Spacing = 8
    { Panel1 Panel
      MoveScaled(30,0,4,2)
      Expand = True
    }
    { btnOK Button
      MoveScaled(45,0,13,3)
      Text = ("OK")
      Default = True
    }
    { btnCancel Button
      MoveScaled(59,0,13,3)
      Text = ("Cancel")
      Cancel = True
    }
  }
}
