' Gambas class file

PUBLIC SUB btnClose_Click()

  ME.Close

END

PUBLIC SUB btnDraw_Click()
  DIM total AS Integer
  total = 10
  WITH FormChart
    .value = NEW Float[]
    .total = total
    .value.Resize(total + 1)
    .value[1] = Val(textbox1.Text)
    .value[2] = Val(textbox2.Text)
    .value[3] = Val(textbox3.Text)
    .value[4] = Val(textbox4.Text)
    .value[5] = Val(textbox5.Text)
    .value[6] = Val(textbox6.Text)
    .value[7] = Val(textbox7.Text)
    .value[8] = Val(textbox8.Text)
    .value[9] = Val(textbox9.Text)
    .value[10] = Val(textbox10.Text)
    .Show
  END WITH

END
