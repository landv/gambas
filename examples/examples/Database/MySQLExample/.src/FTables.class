Public Sub tvwTables_Select()

  If tvwTables.Row = -1 Then Return
  textResult.Clear()
  textResult.Text = modMain.$Connection.MySQL.Table.Info(tvwTables[tvwTables.Row, 0].Text)
  FieldInfo()
  IndexInfo()
  Select (tvwTables.Column)
