PUBLIC SUB timAuthor_Timer()
  DIM Y AS Integer
  Y = svwAuthor.ScrollY
  INC svwAuthor.ScrollY
  IF Y = svwAuthor.ScrollY THEN svwAuthor.ScrollY = 0
END

PUBLIC SUB Button1_Click()
  ME.close
END

PUBLIC SUB Form_Open()
  ME.center
  ME.text="About " & Application.Name
  lblprgname.text=lblprgname.text & Application.name & "  "
  lblversion.text="<h3 align=Right>Version " & Application.Version & "</h3>"
END
