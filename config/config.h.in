#ifndef GAMBAS_MAIN_CONFIG_H
#define GAMBAS_MAIN_CONFIG_H

/* Architecture */
#cmakedefine ARCHITECTURE "${ARCHITECTURE}"

/* Target architecture is ARM */
#cmakedefine ARCH_ARM 1

/* Target architecture is PowerPC */
#cmakedefine ARCH_PPC 1

/* Target architecture is x86 */
#cmakedefine ARCH_X86 1

/* Target architecture is x86_64 */
#cmakedefine ARCH_X86_64 1

/* Do not use libtool to load shared libraries */
#cmakedefine DONT_USE_LTDL 1

/* Full Gambas version */
#define GAMBAS_FULL_VERSION ${GAMBAS_FULL_VERSION}

/* Gambas full version string */
#define GAMBAS_FULL_VERSION_STRING "${GAMBAS_VERSION_MAJOR}.${GAMBAS_VERSION_MINOR}"

/* Gambas minor version */
#define GAMBAS_MINOR_VERSION ${GAMBAS_VERSION_MINOR}

/* Gambas bytecode version */
#define GAMBAS_PCODE_VERSION ${GAMBAS_PCODE_VERSION}

/* Minimum Gambas bytecode version */
#define GAMBAS_PCODE_VERSION_MIN ${GAMBAS_PCODE_VERSION_MIN}

/* Gambas version */
#define GAMBAS_VERSION ${GAMBAS_VERSION_MAJOR}

/* Gambas version string */
#define GAMBAS_VERSION_STRING "${GAMBAS_VERSION_MAJOR}"

/* Define to 1 if you have the `atoll' function. */
#cmakedefine HAVE_ATOLL 1

/* Define to 1 if you have the `cfmakeraw' function. */
#cmakedefine HAVE_CFMAKERAW 1

/* Have libiconv */
#cmakedefine HAVE_CONV_COMPONENT 1

/* Define to 1 if you have the <dirent.h> header file, and it defines `DIR'.
   */
#cmakedefine HAVE_DIRENT_H 1

/* Define to 1 if you have the `daemon' function. */
#cmakedefine HAVE_DAEMON 1

/* Define to 1 if you have the <dlfcn.h> header file. */
#cmakedefine HAVE_DLFCN_H 1

/* Define if you have exp10 function. */
#cmakedefine HAVE_EXP10 1

/* Define if you have exp2 function. */
#cmakedefine HAVE_EXP2 1

/* Have libffi */
#cmakedefine HAVE_FFI_COMPONENT 1

/* Define to 1 if you have the <fcntl.h> header file. */
#cmakedefine HAVE_FCNTL_H 1

/* Define to 1 if you have the `gai_strerror' function. */
#cmakedefine HAVE_GAI_STRERROR 1

/* Whether gcc supports -fvisibility=hidden */
#cmakedefine HAVE_GCC_VISIBILITY 1

/* Define to 1 if you have the `getaddrinfo' function. */
#cmakedefine HAVE_GETADDRINFO 1

/* Define to 1 if you have the `getdomainname' function. */
#cmakedefine HAVE_GETDOMAINNAME 1

/* Define to 1 if you have the `getnameinfo' function. */
#cmakedefine HAVE_GETNAMEINFO 1

/* Define to 1 if you have the `getpagesize' function. */
#cmakedefine HAVE_GETPAGESIZE 1

/* Define to 1 if you have the `getpt' function. */
#cmakedefine HAVE_GETPT 1

/* Define to 1 if you have the <grp.h> header file. */
#cmakedefine HAVE_GRP_H 1

/* Have libgettextlib */
#cmakedefine  HAVE_GETTEXT_COMPONENT 1

/* Have gb.httpd */
#cmakedefine HAVE_HTTPD_COMPONENT 1

/* Have gb.inotify */
#cmakedefine HAVE_INOTIFY_COMPONENT 1

/* if int64_t exists */
#cmakedefine HAVE_INT64T 1

/* Have libintl */
#cmakedefine HAVE_INTL_COMPONENT 1

/* Define to 1 if you have the <inttypes.h> header file. */
#cmakedefine HAVE_INTTYPES_H 1

/* Define to 1 if you have the `kqueue' function. */
#cmakedefine HAVE_KQUEUE 1

/* Define to 1 if you have the `crypt' library (-lcrypt). */
#cmakedefine HAVE_LIBCRYPT 1

/* Define to 1 if you have the `inet6' library (-linet6). */
#cmakedefine HAVE_LIBINET6 1

/* Define if you have log2 function. */
#cmakedefine HAVE_LOG2 1

/* Define to 1 if you have the <memory.h> header file. */
#cmakedefine HAVE_MEMORY_H 1

/* Define to 1 if you have a working `mmap' system call. */
#cmakedefine HAVE_MMAP 1

/* Define to 1 if you have the <ndir.h> header file, and it defines `DIR'. */
#cmakedefine HAVE_NDIR_H 1

/* Define to 1 if you have the <osreldate.h> header file. */
#cmakedefine HAVE_OSRELDATE_H 1

/* Define to 1 if you have the <paths.h> header file. */
#cmakedefine HAVE_PATHS_H 1

/* Define to 1 if you have the `poll' function. */
#cmakedefine HAVE_POLL 1

/* Define to 1 if you have the <poll.h> header file. */
#cmakedefine HAVE_POLL_H 1

/* Define to 1 if you have the `select' function. */
#cmakedefine HAVE_SELECT 1

/* Define to 1 if you have the `setenv' function. */
#cmakedefine HAVE_SETENV 1

/* Define to 1 if you have the `setlogin' function. */
#cmakedefine HAVE_SETLOGIN 1

/* Define to 1 if you have the `setsid' function. */
#cmakedefine HAVE_SETSID 1

/* if socklen_t exists */
#cmakedefine HAVE_SOCKLENT 1

/* Define to 1 if you have the <stdint.h> header file. */
#cmakedefine HAVE_STDINT_H 1

/* Define to 1 if you have the <stdlib.h> header file. */
#cmakedefine HAVE_STDLIB_H 1

/* Define to 1 if you have the `strerror' function. */
#cmakedefine HAVE_STRERROR 1

/* Define to 1 if you have the <strings.h> header file. */
#cmakedefine HAVE_STRINGS_H 1

/* Define to 1 if you have the <string.h> header file. */
#cmakedefine HAVE_STRING_H 1

/* Define to 1 if you have the <sys/devpoll.h> header file. */
#cmakedefine HAVE_SYS_DEVPOLL_H 1

/* Define to 1 if you have the <sys/dir.h> header file, and it defines `DIR'.
   */
#cmakedefine HAVE_SYS_DIR_H 1

/* Define to 1 if you have the <sys/event.h> header file. */
#cmakedefine HAVE_SYS_EVENT_H 1

/* Define to 1 if you have the <sys/ndir.h> header file, and it defines `DIR'.
   */
#cmakedefine HAVE_SYS_NDIR_H 1

/* Define to 1 if you have the <sys/param.h> header file. */
#cmakedefine HAVE_SYS_PARAM_H 1

/* Define to 1 if you have the <sys/poll.h> header file. */
#cmakedefine HAVE_SYS_POLL_H 1

/* Define to 1 if you have the <sys/stat.h> header file. */
#cmakedefine HAVE_SYS_STAT_H 1

/* Define to 1 if you have the <sys/types.h> header file. */
#cmakedefine HAVE_SYS_TYPES_H 1

/* if struct tm has the BSD tm_gmtoff member */
#cmakedefine HAVE_TM_GMTOFF 1

/* Define to 1 if you have the <unistd.h> header file. */
#cmakedefine HAVE_UNISTD_H 1

/* Define to 1 if you have the `unsetenv' function. */
#cmakedefine HAVE_UNSETENV 1

/* Define to 1 if you have the `vsnprintf' function. */
#cmakedefine HAVE_VSNPRINTF 1

/* Define to 1 if you have the `waitpid' function. */
#cmakedefine HAVE_WAITPID 1

/* have __progname */
#cmakedefine HAVE__PROGNAME 1

/* Target system is of BSD family */
#cmakedefine OS_BSD 1

/* Target system is Cygwin/Windows */
#cmakedefine OS_CYGWIN 1

/* Target system is FreeBSD */
#cmakedefine OS_FREEBSD 1

/* Target system is of GNU family */
#cmakedefine OS_GNU 1

/* Target system is Hurd */
#cmakedefine OS_HURD 1

/* Target system is kFREEBSD */
#cmakedefine OS_KFREEBSD 1

/* Target system is Linux */
#cmakedefine OS_LINUX 1

/* Target system is MacOS X */
#cmakedefine OS_MACOSX 1

/* Target system is NetBSD */
#cmakedefine OS_NETBSD 1

/* Target system is OpenBSD */
#cmakedefine OS_OPENBSD 1

/* Target system is Solaris */
#cmakedefine OS_SOLARIS 1

/* Operating system */
#define PACKAGE "${PACKAGE}"

/* Shared library extension */
#define SHARED_LIBRARY_EXT "${SHARED_LIBRARY_EXT}"

/* Operating system */
#define SYSTEM "${SYSTEM}"

/* Version number of package */
#define VERSION "${VERSION}"

#cmakedefine GIT_REVISION "${GIT_REVISION}"

#endif /* GAMBAS_MAIN_CONFIG_H */
