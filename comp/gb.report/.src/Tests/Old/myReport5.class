' Gambas class file

Public Sub _new()

  Dim Rlbl As ReportLabel
  Dim i As Integer
  'Report.Debug = True
  For i = 0 To 100

    Rlbl = New ReportLabel(RVBCont)
    Rlbl.Autoresize = True
    Rlbl.Text = "Ligne " & i
    Rlbl.Border.bottom.Width = "1px"
    Rlbl.Padding.Top = "3 mm"
    Rlbl.Padding.Bottom = "1mm"
    Rlbl.Padding.Left = "2 mm"
    Rlbl.Top = i & "cm"
    Rlbl.Left = i & "cm"
    Rlbl.Width = "1 cm"
  Next

End
